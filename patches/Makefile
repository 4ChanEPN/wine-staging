.PHONY: install
install:
	@$(MAKE) apply; \
	status=$$?;     \
	rm -f *.ok;     \
	exit $$status

.PHONY: apply
apply: $(PATCHLIST)
	cat *.ok | sort | $(CURDIR)/../debian/tools/patchlist.sh | $(PATCH)
	cd $(DESTDIR) && autoreconf -f
	cd $(DESTDIR) && ./tools/make_requests

.PHONY: clean
clean:
	rm -f *.ok

